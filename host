    -- Update personal info
    PersonalDisplayName.Text = PLAYER.DisplayName or PLAYER.Name
    PersonalUsername.Text = "<i>@"..PLAYER.Name.."</i>"
    PersonalCash.Text = "Cash - <font color='rgb(0,255,0)'>$"..commaValue(PLAYER_CASH.Value or 0).."</font>"

    -- Update cash when changed
    if PLAYER_CASH then
        PLAYER_CASH.Changed:Connect(function(newValue)
            PersonalCash.Text = "Cash - <font color='rgb(0,255,0)'>$"..commaValue(newValue or 0).."</font>"
        end)
    end

    -- Functions for updating alt list
    local function updateAltsList()
        altsInGame = getAltsInGame()
        
        -- Clear existing frames
        for _, child in ipairs(playersScrollFrame:GetChildren()) do
            if child:IsA("Frame") and child.Name ~= "playersScrollListLayout" and child.Name ~= "playersScrollFrameUIPadding" then
                child:Destroy()
            end
        end
        
        -- Create new frames for alts
        for _, altPlayer in ipairs(altsInGame) do
            createAltFrame(altPlayer)
        end
        
        -- Update canvas size
        playersScrollFrame.CanvasSize = UDim2.new(0, 0, 0, #altsInGame * 125)
        commandsScrollFrame.CanvasSize = UDim2.new(0, 0, 0, #commands * 50)
        settingsScrollFrame.CanvasSize = UDim2.new(0, 0, 0, #settings * 60)
    end

    -- Initial update
    updateAltsList()

    -- Update when players join/leave
    Players.PlayerAdded:Connect(updateAltsList)
    Players.PlayerRemoving:Connect(updateAltsList)

    -- Recount cash button
    recountAllCashBtn.MouseButton1Down:Connect(function()
        -- Update cash displays
        PersonalCash.Text = "Cash - <font color='rgb(0,255,0)'>$"..commaValue(PLAYER_CASH.Value or 0).."</font>"
        
        local totalAltCash = 0
        local totalFloorCash = 0
        
        for _, altPlayer in ipairs(altsInGame) do
            local altFrame = playersScrollFrame:FindFirstChild(altPlayer.Name)
            if altFrame and altPlayer.leaderstats and altPlayer.leaderstats.Money then
                local altCash = altFrame:FindFirstChild("altCash")
                local altFloorCash = altFrame:FindFirstChild("altFloorCash")
                if altCash then
                    altCash.Text = "Cash - <font color='rgb(0,255,0)'>$"..commaValue(altPlayer.leaderstats.Money.Value or 0).."</font>"
                    totalAltCash = totalAltCash + (altPlayer.leaderstats.Money.Value or 0)
                end
                if altFloorCash then
                    altFloorCash.Text = "Floor Cash - <font color='rgb(0,255,0)'>$0</font>"
                end
            end
        end
        
        local playerCashValue = PLAYER_CASH.Value or 0
        AltsTotalCash.Text = "Total Alts Cash - <font color='rgb(0,255,0)'>$"..commaValue(totalAltCash).."</font>"
        CombinedTotalCash.Text = "Combined Cash - <font color='rgb(0,255,0)'>$"..commaValue(playerCashValue + totalAltCash).."</font>"
        CombinedFloorCash.Text = "Combined Floor Cash - <font color='rgb(0,255,0)'>$"..commaValue(totalFloorCash).."</font>"
        CombinedTotalNet.Text = "Net Worth - <font color='rgb(0,255,0)'>$"..commaValue(playerCashValue + totalAltCash + totalFloorCash).."</font>"
    end)

    print("HOST GUI Loaded Successfully!")
-- end
