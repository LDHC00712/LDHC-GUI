    -- Create settings
    local settings = {
        {name = "Auto Recount Cash", description = "Automatically update cash counts"},
        {name = "Show Floor Cash", description = "Display floor cash for each alt"},
        {name = "Auto Setup On Join", description = "Auto setup new alts"},
        {name = "Debug Mode", description = "Show debug information"}
    }

    for i, setting in ipairs(settings) do
        local settingFrame = Instance.new("Frame")
        settingFrame.Name = "settingFrame"..i
