    -- Create commands in the GUI
    local commands = {
        {name = ".drop", description = "Toggle auto dropping for all alts"},
        {name = ".cdrop [amount]", description = "Drop specific amount (e.g. 1k, 1m, 1b)"},
        {name = ".aura", description = "Toggle cash aura for all alts"},
        {name = ".setup [location]", description = "Setup alts at location (admin, sewers, bank, hood)"},
        {name = ".setup [location] og", description = "Setup alts in line formation"},
        {name = ".setup host", description = "Teleport all alts to your position"},
        {name = ".stop", description = "Stop all alt activities"},
        {name = ".destroycash", description = "Destroy floor cash for all alts"},
        {name = ".dropped", description = "Get floor cash count from all alts"}
    }

    for i, cmd in ipairs(commands) do
        local cmdFrame = Instance.new("Frame")
        cmdFrame.Name = "cmdFrame"..i
        cmdFrame.LayoutOrder = i
        cmdFrame.BorderColor3 = Color3.new(0.105882, 0.164706, 0.207843)
        cmdFrame.BackgroundTransparency = 1
        cmdFrame.BackgroundColor3 = Color3.new(1, 1, 1)
        cmdFrame.Size = UDim2.new(1, 0, 0, 45)
