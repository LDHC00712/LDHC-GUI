-- Initialize required services and variables
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local PLAYER = Players.LocalPlayer

-- Initialize getgenv if not exists
if not getgenv().alts then
    getgenv().alts = {}
end
if not getgenv().mainId then
    getgenv().mainId = PLAYER.UserId
end

-- Wait for leaderstats safely
local function waitForLeaderstats()
    local leaderstats = PLAYER:FindFirstChild("leaderstats")
    if not leaderstats then
        PLAYER.ChildAdded:Wait()
        leaderstats = PLAYER:FindFirstChild("leaderstats")
    end
    
    local money = leaderstats and leaderstats:FindFirstChild("Money")
    if not money then
        -- Create a default money value if it doesn't exist
        money = Instance.new("NumberValue")
        money.Name = "Money"
        money.Value = 0
        if leaderstats then
            money.Parent = leaderstats
        end
    end
    return money
end

local PLAYER_CASH = waitForLeaderstats()

-- Utility function for formatting numbers
local function commaValue(amount)
    local formatted = tostring(amount)
    while true do
        formatted, k = string.gsub(formatted, "^(-?%d+)(%d%d%d)", '%1,%2')
        if (k==0) then
            break
        end
    end
    return formatted
end

-- Check if this is the HOST account (main account)
local function isAlt(userId)
    if not getgenv().alts or #getgenv().alts == 0 then
        return false
    end
    for i, id in ipairs(getgenv().alts) do
        if userId == id then
            return true
        end
    end
    return false
end

