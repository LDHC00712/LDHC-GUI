    --VARIABLES
    --get all alts in game
    local altsInGame = {}
    local function getAltsInGame()
        local localAltsInGame = {}
        for _, altUserId in pairs(getgenv().alts) do
            for _, player in pairs(Players:GetPlayers()) do
                if player.UserId == altUserId then
                    table.insert(localAltsInGame, player)
                end
            end
        end
        return localAltsInGame
    end
    altsInGame = getAltsInGame()

    local altConnections = {}
    local connectionCleanup = {}

